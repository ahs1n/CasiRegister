                            android:layout_height="wrap_content"
                            android:hint="@string/cb13"
                            android:inputType="number"
                            android:maxLength="2"
                            android:text="@={childInformation.cb13}"
                            app:maxValue="17"
                            app:minValue="1"
                            app:type="range" />
                    </LinearLayout>
                </androidx.cardview.widget.CardView>


                <!-- cb14  Input   -->
                <androidx.cardview.widget.CardView
                    android:id="@+id/fldGrpCVcb14"
