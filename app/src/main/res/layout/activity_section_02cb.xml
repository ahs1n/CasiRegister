                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/bottom">

                            <TextView
                                android:id="@+id/qtxt_cb14"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_toEndOf="@id/q_cb14"
                                android:text="@string/cb14" />

                            <TextView
                                android:id="@+id/q_cb14"
                                style="@style/quesNum"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignTop="@id/qtxt_cb14"
                                android:layout_alignBottom="@id/qtxt_cb14"
                                android:text="@string/Q_cb14" />
                        </RelativeLayout>

                        <EditText
                            android:id="@+id/cb14"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="@string/cb14"
                            android:text="@={childInformation.cb14}" />
                    </LinearLayout>
                </androidx.cardview.widget.CardView>


                <!-- cb15  Input-Numeric   -->
                <androidx.cardview.widget.CardView
                    android:id="@+id/fldGrpCVcb15"
                    style="@style/cardView">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/bottom">

                            <TextView
                                android:id="@+id/qtxt_cb15"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_toEndOf="@id/q_cb15"
                                android:text="@string/cb15" />

                            <TextView
                                android:id="@+id/q_cb15"
                                style="@style/quesNum"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignTop="@id/qtxt_cb15"
                                android:layout_alignBottom="@id/qtxt_cb15"
                                android:text="@string/Q_cb15" />
                        </RelativeLayout>

                        <com.edittextpicker.aliazaz.EditTextPicker
                            android:id="@+id/cb15"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:hint="@string/cb15"
                            android:inputType="number"
                            android:text="@={childInformation.cb15}"
                            app:maxValue="999999"
                            app:minValue="1"
                            app:type="range" />
                    </LinearLayout>
                </androidx.cardview.widget.CardView>


                <!-- cb16  Radio   -->
                <androidx.cardview.widget.CardView
                    android:id="@+id/fldGrpCVcb16"
                    style="@style/cardView">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/bottom">

                            <TextView
                                android:id="@+id/qtxt_cb16"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_toEndOf="@id/q_cb16"
                                android:text="@string/cb16" />

                            <TextView
                                android:id="@+id/q_cb16"
                                style="@style/quesNum"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignTop="@id/qtxt_cb16"
                                android:layout_alignBottom="@id/qtxt_cb16"
                                android:text="@string/Q_cb16" />
                        </RelativeLayout>

                        <RadioGroup
                            android:id="@+id/cb16"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <RadioButton
                                android:id="@+id/cb1601"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:checked="@{childInformation.cb16.equals(`1`)}"
                                android:onClick="@{()->childInformation.setCb16(`1`)}"
                                android:text="@string/cb1601" />

                            <RadioButton
                                android:id="@+id/cb1602"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:checked="@{childInformation.cb16.equals(`2`)}"
                                android:onClick="@{()->childInformation.setCb16(`2`)}"
                                android:text="@string/cb1602" />

                            <RadioButton
                                android:id="@+id/cb1603"
